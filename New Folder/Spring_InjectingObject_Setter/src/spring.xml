<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="addressObj" class="com.Address">
	<property name="address" value="UK"></property>
	</bean>
   <bean id ="emp" class = "com.Employee">
	<property name="name" value="SHOEB"></property>
 	<property  name="address" ref="addressObj"></property>
 	<property name="SSN" value="10123"></property>
 
   </bean>
   
</beans>

